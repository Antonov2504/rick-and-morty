{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/preset-typescript"
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-modules-commonjs"
    ],
    [
      "@babel/plugin-transform-object-assign"
    ],
    ["babel-plugin-styled-components"]
  ]
}